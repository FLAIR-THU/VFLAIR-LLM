{
  "epochs": 2,
  "lr": 0.0001,
  "k": 2,
  "batch_size": 8,
  "test_batch_size": 1,
  "early_stop_threshold": 1,
  "pipeline": "finetune",
  "model_architect":"CLM",
  "communication":{
      "communication_protocol": "FedSGD",
      "iteration_per_aggregation": 1
  },
  "finetune_configs":{
      "name":"Vanilla",
      "configs":{
          "inference_mode":false,  
        "r":4,  
        "lora_alpha":32, 
        "lora_dropout":0.1
      }
  },
  "dataset":{
      "dataset_name": "GMS8K",
      "num_classes": 1,
      "use_prompt": 1
  },
  "tokenizer":{
      "padding": "max_length",
      "padding_type": "outside",
      "pad_token": "default",
      "truncation": "True",
      "max_length": 128,
      "padding_side": "left",
      "add_special_tokens": 1
  },
  "model_list":{
      "name": "googleflan-t5-small",
      "pretrained": 1,
      "model_type": "T5",
      "max_sequence": 512,
      "kwargs_model_loading":{
          "device_map": "auto"
      },
      "0": {
          "path": "/shared/model/googleflan-t5-small",
              "head":{
                  "trainable": 1,
                  "embedding_trainable": 0,
                  "encoder_trainable": 0,
                  "encoder_trainable_ids":[]
              },
              "tail":{
                  "trainable": 1,
                  "head_layer_trainable": 0,
                  "encoder_trainable": 0,
                  "encoder_trainable_ids":[]}
      },
      "1": {
              "path": "/shared/model/googleflan-t5-small",
              "body":{
                  "trainable": 0,
                  "encoder_trainable": 0,
                  "encoder_trainable_ids":[]
          }
      },
      "task":{
          "task_type": "CausalLM",
          "generation_config_dict":{
              "temperature":1.0,
              "top_k": -1,
              "top_p": 1.0,
              "max_new_tokens": 50
          },
          "max_new_tokens": 50
      },
      "vfl_model_slice_num": 3,
      "local_encoders_num": 2,
      "local_tail_encoders_num": 2
  }
}
