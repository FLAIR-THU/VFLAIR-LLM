{
    "epochs": 40,
    "lr": 1e-6,
    "k": 2,
    "batch_size": 32,
    "early_stop_threshold": 2,
    "pipeline": "finetune",
    "model_architect":"CLS",
    "communication":{
        "communication_protocol": "FedSGD",
        "iteration_per_aggregation": 1
    },
    "dataset": {
      "dataset_name": "CoLA",
      "num_classes": 2
    },
    "tokenizer": {
      "padding": "max_length",
      "padding_type": "outside",
      "pad_token": "[PAD]",
      "truncation": "longest_first",
      "max_length": 70,
      "padding_side": "left",
      "add_special_tokens": 1
    },
    "finetune_configs":{
        "name":"Vanilla",
        "configs":{}
    },
    "model_list":{
        "name": "textattackbert-base-uncased-CoLA",
        "pretrained": 1,
        "model_type": "Bert",
        "max_sequence": 512,
        "kwargs_model_loading":{
        },
        "0": {
          "path": "/shared/model/textattackbert-base-uncased-CoLA",
  
            "head":{
                "trainable": 1
            },
            "tail":{
                "trainable": 1
            }
        },
        "1": {
          "path": "/shared/model/textattackbert-base-uncased-CoLA",
            "body":{
                "trainable": 1
            }
        },
        "task":{
            "task_type": "SequenceClassification"
        },
        "vfl_model_slice_num": 3,
        "local_encoders_num": 3,
        "local_tail_encoders_num": 3
        
    }
  }
  
  